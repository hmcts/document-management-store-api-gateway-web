{
  "name": "document-management-store-api-gateway-web",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_PATH=. node server.js",
    "start-dev": "cross-env NODE_PATH=. node-dev server.js",
    "setup": "cross-env NODE_PATH=. npm run sass",
    "sass": "cross-env NODE_PATH=. node-sass app/assets/sass/*.scss --output app/assets/public/css --quiet --output-style expanded --source-map true --include-path node_modules/govuk_template_jinja/assets/stylesheets --include-path node_modules/govuk_frontend_toolkit/stylesheets --include-path node_modules/govuk-elements-sass/public/sass/",
    "lint": "eslint ./app/**/*.js && eslint ./spec/**/*.js",
    "test": "nyc --all --reporter=lcov --reporter=text-lcov mocha ./spec/**/*.spec.js --reporter mochawesome",
    "e2e": "echo 'e2e not implemented'",
    "test-all": "echo 'test all not implemented'",
    "test:a11y": "echo 'ally test not implemented'",
    "test:coverage": "npm test && nyc check-coverage --statements 80 --lines 80",
    "test:nsp": "nsp check",
    "test:nsp-warn": "nsp check --output summary --warn-only",
    "test:snyk": "snyk test",
    "test:smoke": "echo 'smoke test not implemented'",
    "test:functional": "echo 'functional test not implemented'",
    "test:codacy-upload": "cat ./coverage/lcov.info | codacy-coverage",
    "test:codecov-upload": "codecov -f ./coverage/lcov.info",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "health-check": "echo 'healthcheck not implemented'",
    "clean": "rm -r node_modules && rm -r mochawesome-report && rm -r coverage && rm -r  .nyc_output",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@hmcts/nodejs-healthcheck": "^1.4.6",
    "@hmcts/nodejs-logging": "^1.4.2",
    "config": "^1.27.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "express-nunjucks": "^2.2.3",
    "express-validator": "^5.0.0",
    "form-data": "^2.3.1",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.1.0",
    "govuk_template_jinja": "^0.23.0",
    "helmet": "^3.9.0",
    "js-yaml": "^3.10.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.5.3",
    "nunjucks": "^3.0.1",
    "otp": "^0.1.3",
    "serve-favicon": "^2.4.5",
    "yarn": "1.2.1",
    "snyk": "^1.69.10"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "codacy-coverage": "^2.0.3",
    "chai": "^4.0.2",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "git-message": "^2.0.2",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "^1.15.0",
    "mochawesome": "^2.3.1",
    "nsp": "^2.6.3",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0",
    "sonar-scanner": "^1.0.1"
  },
  "nyc": {
    "exclude": [
      "mochawesome-report",
      "coverage",
      "spec"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "coverage",
    "all": true
  },
  "snyk": true
}
